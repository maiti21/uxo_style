/* PLOT */
way[Plot] {
   	width: 4;
	dashes: 15,7;
    /*dashes-background-color: #00ff00;*/
}

way[Plot] > node {
	symbol-shape: square;
	symbol-size: 5;
	symbol-fill-opacity: 0;
 	symbol-stroke-color:#D8D8D8;
}

way[Plot] > node:connection {
symbol-shape: circle;
symbol-fill-color: yellow;
symbol-fill-opacity: 1;
symbol-size:10;

}


/* ERRORES */
way[Comment] {
    width: 3;
    color: blue;
	fill-color: blue;
	fill-opacity: 0.1;
}

node:unconnected,
node:tagged,
way[Comment] > node {
	symbol-shape: square;
	symbol-size: 8;
	symbol-fill-opacity: 1;
	symbol-fill-color: #2E64FE;
    symbol-stroke-color: red;
    symbol-stroke-width: 2;
}

way[Land_use]!:closed>[index=-1]node,
way[Land_use]!:closed>[index=1]node{
   symbol-shape: circle;
   symbol-stroke-color: red;
   symbol-stroke-width: 5;
   symbol-fill-color: orange;
   symbol-fill-opacity: 1;
   symbol-size: 15;
}

way[ID_string]!:closed>[index=-1]node!:connection,
way[ID_string]!:closed>[index=1]node!:connection 
{
   symbol-shape: circle;
   symbol-stroke-color: red;
   symbol-stroke-width: 5;
   symbol-fill-color: orange;
   symbol-fill-opacity: 1;
   symbol-size: 15;
}
node[ID_string]
{
   symbol-shape: circle;
   symbol-stroke-color: yellow;
   symbol-stroke-width: 2;
   symbol-fill-color: blue;
   symbol-fill-opacity: 0.65;
   symbol-size: 10;
}
node:selected {
    symbol-shape: circle;
    symbol-size: 10;
    symbol-fill-color: node_selected#ff0000;
    symbol-stroke-color: node_selected#ff0000;
}
way[Width = "Narrow"] {
   	width: 1.5;
	dashes: 20,4;
	/*dashes-background-color: #0000FF;*/
}
way[Width= "Narrow"] > node {
	symbol-shape: square;
	symbol-size: 4;
 	symbol-stroke-color:#0000FF;
}
way[Narrow]> node:connection {
symbol-shape: circle;
symbol-fill-color: blue;
symbol-fill-opacity: 1;
symbol-size:15;
}

way[Width = "Wide"] {
	width: 3;
	/*dashes-background-color: #99FF00;*/
}
way[Width= "Wide"] > node {
	symbol-shape: square;
	symbol-size: 4;
	symbol-stroke-color: #99FF00;
}
way[Wide] > node:connection {
symbol-shape: circle;
symbol-fill-color: green;
symbol-fill-opacity: 1;
symbol-size:15;
}
